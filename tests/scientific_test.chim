// Chim测试程序 - 科学计算场景
// 测试Fortran后端和汇编后端

// 简单的数值计算
fn compute(x: int, y: int) -> int = {
    let tmp1: int = x * y;
    let tmp2: int = tmp1 + x;
    let result: int = tmp2 - y;
    return result
}

// 加法运算
fn add(a: int, b: int) -> int = {
    return a + b
}

// 乘法运算
fn multiply(a: int, b: int) -> int = {
    return a * b
}

// 比较运算
fn max(a: int, b: int) -> int = {
    if a > b {
        return a
    } else {
        return b
    }
}

fn main() -> int = {
    let a: int = 10;
    let b: int = 20;
    let result: int = compute(a, b);
    return result
}
