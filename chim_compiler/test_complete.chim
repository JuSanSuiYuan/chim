fn add(a: int, b: int) -> int {
    return a + b;
}

fn subtract(a: int, b: int) -> int {
    return a - b;
}

fn multiply(a: int, b: int) -> int {
    return a * b;
}

fn divide(a: int, b: int) -> int {
    return a / b;
}

fn modulo(a: int, b: int) -> int {
    return a % b;
}

fn main() {
    let sum = add(10, 20);
    let diff = subtract(30, 15);
    let prod = multiply(5, 6);
    let quot = divide(100, 10);
    let rem = modulo(17, 5);
}
